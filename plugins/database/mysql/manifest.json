{
  "name": "mysql",
  "version": "1.0.0",
  "kind": "database",
  "pluginType": "builtin",
  "displayName": {
    "en-US": "MySQL Database",
    "zh-CN": "MySQL 数据库"
  },
  "description": {
    "en-US": "MySQL database plugin with query editor and data viewer",
    "zh-CN": "MySQL 数据库插件，支持查询编辑器和数据查看器"
  },
  "author": "baibai",
  "homepage": "https://github.com/baibai/mysql-plugin",
  "icon": "mysql.svg",
  "capabilities": [
    "transaction",
    "prepared_statement",
    "batch_operation",
    "stored_procedure"
  ],
  "i18n": {
    "defaultLocale": "zh-CN",
    "supportedLocales": ["zh-CN", "en-US"],
    "resources": "./i18n"
  },
  "native": {
    "library": "mysql_plugin"
  },
  "ui": {
    "entry": "./dist/index.js"
  }
}
